{
  "name": "mj-famous",
  "version": "0.3.0-alpha.1",
  "description": "seed for a browserified version of famous",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run lint -s",
    "build": "browserify src/index.js -t cssify -t brfs -o famous.js --debug --verbose",
    "watch": "watchify src/index.js -t cssify -t brfs -o famous.js --debug --verbose",
    "lint-eslint": "eslint src/",
    "lint-jscs": "jscs src/",
    "lint": "npm run lint-eslint; npm run lint-jscs"
  },
  "devDependencies": {
    "eslint": "^0.7.4",
    "jscs": "^1.5.8"
  },
  "author": "TheAlphaNerd",
  "license": "ISC",
  "dependencies": {
    "brfs": "^1.1.1",
    "browserify": "^5.9.1",
    "cssify": "^0.5.1",
	  "famous": "git://github.com/Famous/famous#4f7b60b7944ee53829713b02b818a9a384c2e351",
    "famous-polyfills": "0.2.2",
    "watchify": "^1.0.1"
  }
}
