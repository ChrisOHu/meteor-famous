{
  "name": "mj-famous",
  "version": "0.2.2",
  "description": "seed for a browserified version of famous",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run lint -s",
    "build": "browserify src/index.js -t cssify -t brfs -o famous.js --debug --verbose",
    "watch": "watchify src/index.js -t cssify -t brfs -o famous.js --debug --verbose",
    "lint-eslint": "eslint src/",
    "lint-jscs": "jscs src/",
    "lint": "npm run lint-eslint; npm run lint-jscs"
  },
  "devDependencies": {
    "eslint": "^0.7.4",
    "jscs": "^1.5.8"
  },
  "author": "TheAlphaNerd",
  "license": "ISC",
  "dependencies": {
    "brfs": "^1.1.1",
    "browserify": "^5.9.1",
    "cssify": "^0.5.1",
    "famous": "Famous/famous#d28f17dc94e5d7b0a1f0e2ba5df3938ff29d9900",
    "famous-polyfills": "^0.2.2",
    "watchify": "^1.0.1"
  }
}
